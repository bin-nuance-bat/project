{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/!(admin|training|preview|itemapproval)",
        "destination": "/",
        "type": 301
      }
    ],
    "rewrites" :[
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
