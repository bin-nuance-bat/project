{
	"name": "honesty-store-kiosk",
	"description": "Kiosk client for honesty store in scottLogic NCL office",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@tensorflow-models/mobilenet": "^0.1.1",
		"@tensorflow/tfjs": "^0.11.7",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-scripts": "1.1.4",
		"react-webcam": "^1.0.1"
	},
	"scripts": {
		"prettify": "prettier --write ./src/**/*.js",
		"puppeteer-test": "node test/puppeteer.js",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"deploy": "react-scripts build && firebase deploy --token $FIREBASE_TOKEN"
	},
	"lint-staged": {
		"*.js": [
			"npm run prettify",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm run puppeteer-test"
		}
	},
	"devDependencies": {
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"husky": "^1.0.0-rc.10",
		"lint-staged": "^7.2.0",
		"prettier": "^1.13.7",
		"puppeteer": "^1.5.0"
	},
	"eslintConfig": {
		"parserOptions": {
			"ecmaVersion": 9,
			"sourceType": "module",
			"ecmaFeatures": {
				"jsx": true
			}
		}
	}
}
